<?xml version="1.0" encoding="UTF-8" ?>
<gateway-config xmlns="http://xmlns.kaazing.org/2016/06/gateway">
  
  <service>
    <name>proxy service D</name>
    <accept>ws://nginx:8080/tcpproxy</accept>
    <connect>tcp://server-d:4441</connect>
    <type>proxy</type>
    <accept-options>
      <ws.inactivity.timeout>55 seconds</ws.inactivity.timeout>
      <http.transport>tcp://example.com:7771</http.transport>
    </accept-options>
  </service>

  <service>
    <name>proxy service C</name>
    <accept>tcp://@eth0:8882</accept>
    <connect>pipe://serviceC</connect>
    <type>proxy</type>
    <connect-options>
      <pipe.transport>socks://example.com</pipe.transport>
      <socks.mode>reverse</socks.mode>
      <socks.transport>ws://nginx:8081/tcpreverseproxy/</socks.transport>
      <http.transport>tcp://example.com:7772</http.transport>
      <ws.inactivity.timeout>55 seconds</ws.inactivity.timeout>
    </connect-options>
  </service> 

</gateway-config>
