<?xml version="1.0" encoding="UTF-8" ?>
<gateway-config xmlns="http://xmlns.kaazing.org/2016/06/gateway">

  <service>
    <name>proxy service D</name>
    <accept>tcp://@eth0:8881</accept>
    <connect>ws://nginx:8080/tcpproxy</connect>
    <type>proxy</type>
    <connect-options>
      <ws.inactivity.timeout>10000 seconds</ws.inactivity.timeout>
    </connect-options>
  </service>

  <service>
    <name>proxy service C</name>
    <accept>pipe://serviceC</accept>
    <connect>tcp://server-c:4443</connect>
    <type>proxy</type>
    <accept-options>
      <pipe.transport>socks://example.com</pipe.transport>
      <socks.mode>reverse</socks.mode>
      <socks.transport>ws://nginx:8081/tcpreverseproxy/</socks.transport>
      <ws.inactivity.timeout>55 seconds</ws.inactivity.timeout>
    </accept-options>
  </service> 

</gateway-config>
